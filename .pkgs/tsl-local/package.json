{
  "name": "@local/tsl-local",
  "version": "0.0.0",
  "private": true,
  "description": "Local tsl rules for use in the workspace",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "build:docs": "typedoc",
    "lint:publish": "publint",
    "lint:ts": "tsl"
  },
  "dependencies": {
    "string-ts": "^2.3.1",
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "@local/configs": "workspace:*",
    "@local/eff": "workspace:*",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "tsdown": "^0.20.1"
  },
  "peerDependencies": {
    "tsl": "^1.0.28",
    "typescript": "*"
  }
}
